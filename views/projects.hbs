<div class="container">
    <div class="row">
        <h1>Open Projects</h1>
    </div>


    <div class="row">
        {{#each projects}}
            <div class="col">
                <div class="card mb-3">
                    <h3 class="card-header"> </h3>
                    <div class="card-body">
                        <p class="card-text">Project Name: {{this.projectName}}</p>
                    </div>
                    <div class="card-body">
                        <p class="card-text">Description: {{this.projectDescription}}</p>
                    </div>
                </div>
            </div>
        {{/each}}

    </div>
</div>
