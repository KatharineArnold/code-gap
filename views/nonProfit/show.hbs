<div class="container">
    <div class="row">
        <h1 class="userTitle">{{nonProfitProfile.companyName}} Projects</h1>
    </div>
    <div class="row">
        {{#each nonProfitProfile.Projects}}
            <div class="col-md-4">
                <div class="card mb-3 projectCard">
                    <div class="card-body ">
                        <p class="card-text"><span class="cardTitle">Project Name:</span> {{projectName}}</p>
                    </div>
                    <div class="card-body">
                        <p class="card-text"><span class="cardTitle">Description:</span> {{projectDescription}}</p>
                    </div>
                </div>
            </div>
        {{/each}}

    </div>

</div>
